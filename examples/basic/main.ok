type Test = struct {
    i64 a, 
}

type Animal = enum {
    Dog { u8 size },
    Fish,
    Octopus,
}

fun main(): i64 {
    let a = 10;
    let b = 32;

    let out = 0;
    
    sum_i64(a, b, &out);

    let test = Test { a = 42 };

    let octopus = Animal:Octopus;
    let dog = Animal:Dog { size = 5 };

    ret test.a;
}

fun sum_i64(i64 x, i64 y, &i64 res) {
    *res = x + y;
}
