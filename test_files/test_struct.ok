
struct test2 {
    i32 a,
    test1 b,
}

pub struct test1 {
    i32 a,
    bool b,
    u8 my_byte,
}

fun main(): i8 {
    test1 my_test1 = test1 { 
        a = 42, 
        my_byte = 10, 
        b = true,
        # c = 10,  # error
    };

    i32 a = my_test1.a;

    test2 my_test2 = test2 {
        a = 8,
        b = my_test1, # `my_test1` get's copied to `b`
    };

    i32 a = my_test2.b.a + my_test2.a * my_test1.a;

    print_i32(a);
    println();
}
extern fun print_i32(i32): i16; 
extern fun println(): i16;
